{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "frontend-template",
  "version": "0.0.1",
  "description": "frontend application template",
  "type": "module",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build:app": "nx build my-world-app",
    "build": "nx run-many -t build",
    "test": "nx run-many -t test",
    "dev": "pnpm build && pnpm --recursive --parallel --stream run dev",
    "lint": "nx run-many -t lint",
    "migrate": "nx migrate latest",
    "graph": "nx graph",
    "clear": "nx run-many -t clear",
    "clear:all": "nx run-many -t clear:all && rimraf .nx && rimraf pnpm-lock.* && rimraf node_modules",
    "start:world": "nx start my-world-app"
  },
  "keywords": [],
  "author": {
    "email": "contact@jhasuraj.com",
    "name": "Suraj Jha",
    "url": "https://jhasuraj.com"
  },
  "contributors": [],
  "license": "MIT",
  "engines": {
    "node": "20.11.0",
    "pnpm": "8.15.1"
  },
  "engineStrict": true,
  "packageManager": "pnpm@8.15.1",
  "devDependencies": {
    "@nx/eslint": "18.0.4",
    "eslint": "~8.48.0",
    "nx": "18.0.4",
    "rimraf": "^5.0.5"
  }
}
